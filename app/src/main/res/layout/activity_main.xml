<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="myandroidtest.test.MainActivity">



    <ImageView
        android:id="@+id/title_download"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@android:drawable/btn_star"
        tools:layout_editor_absoluteX="75dp"
        tools:layout_editor_absoluteY="130dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintBottom_toTopOf="@id/title_download"
        app:layout_constraintRight_toLeftOf="@id/title_exp"
        />

    <ImageView
        android:id="@+id/title_exp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@android:drawable/btn_dialog"
        tools:layout_editor_absoluteX="41dp"
        tools:layout_editor_absoluteY="128dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toRightOf="@id/title_download"
        app:layout_constraintRight_toLeftOf="@id/title"
        />
    <TextView
        android:id="@+id/title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ellipsize="end"
        android:text="Hello World! SDFSDFDSFDFSFSDFSSDFSDFDSFSDFSDFSDFSDFSDFSDFSDFSDFSF"
        app:layout_constraintLeft_toRightOf="@id/title_exp"
        app:layout_constraintTop_toTopOf="parent"
        android:maxLines="1"
        />
    <ImageView
        android:id="@+id/subtitle_download"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@android:drawable/btn_star"
        tools:layout_editor_absoluteX="75dp"
        tools:layout_editor_absoluteY="130dp"
        app:layout_constraintTop_toBottomOf="@id/title_download"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@id/subtitle_exp"
        />

    <ImageView
        android:id="@+id/subtitle_exp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@android:drawable/btn_dialog"
        tools:layout_editor_absoluteX="41dp"
        tools:layout_editor_absoluteY="128dp"
        app:layout_constraintTop_toBottomOf="@id/title_exp"
        app:layout_constraintLeft_toRightOf="@+id/subtitle_download"/>



    <TextView
        android:id="@+id/subtitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ellipsize="end"
        android:text="Hello WorldSDFDSFSDFDSFDSFDSFDSFSDFDFDSFDFSDFDSFDS!"
        app:layout_constraintTop_toBottomOf="@id/title"
        app:layout_constraintLeft_toRightOf="@id/subtitle_exp"
        android:maxLines="1" />

